{
  "$schema": "http://json.schemastore.org/template",
  "author": "Muskan Kumari",
  "classifications": ["Web", "Service", "Clean Architecture", "Microservice"],
  "identity": "CleanArchitecture.Template",
  "name": "Clean Architecture Microservice Solution",
  "shortName": "cleanarch",
  "sourceName": "Template",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "description": "A Clean Architecture based Microservice template for .NET",
  "defaultName": "CleanArchService",
  "primaryOutputs": [{ "path": "Template.sln" }],
  "symbols": {
    "ServiceName": {
      "type": "parameter",
      "datatype": "string",
      "description": "Name of your service (e.g. Template, Order, Basket)",
      "replaces": "Template",
      "defaultValue": "name"
    }
  },
  "icon": "Icon2.png"
}
